{
  "private": false,
  "name": "xns-seek-bar",
  "version": "2.0.0",
  "description": "A seekable progress bar component for Vue 3 with TypeScript support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "author": "James Sinkala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xinnks/xns-seek-bar/issues"
  },
  "homepage": "https://github.com/xinnks/xns-seek-bar#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinnks/xns-seek-bar.git"
  },
  "keywords": [
    "seek",
    "slider",
    "progress",
    "vue",
    "vue3",
    "typescript",
    "composition-api",
    "xinnks"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "typescript": "^5.2.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.2.12"
  }
}
